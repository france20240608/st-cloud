<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.st.cloud.module.agent.mapper.SystemSiteMapper">
    <select id="getTenantId" resultType="com.st.cloud.module.agent.pojo.ob.SystemSiteDO">
        SELECT
            *
        FROM
            system_site
        WHERE
            domain = #{domain}
            and type = 1
            and status = 1
    </select>
</mapper>