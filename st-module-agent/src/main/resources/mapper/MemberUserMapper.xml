<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.st.cloud.module.agent.mapper.MemberUserMapper">
    <select id="getByUsername" resultType="com.st.cloud.module.agent.pojo.ob.MemberUserDO">
        SELECT
            id,
            username,
            password,
            login_status,
            bet_status,
            last_login_time,
            tenant_id,
            deleted,
            create_time,
            create_user,
            update_time,
            update_user
        FROM
            member_user
        WHERE
            username=#{username}
    </select>
</mapper>